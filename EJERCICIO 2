#include <iostream>
using namespace std;

int main() {
    int numeros[5] = {45, 12, 78, 3, 56};
    int minimo;
    
 
    minimo = numeros[0];
    
  
    for(int i = 1; i < 5; i++) {
        if(numeros[i] < minimo) {
            minimo = numeros[i];  
        }
    }
    
    cout << "El numero mas pequeno es: " << minimo << endl;
    
    return 0;
}
